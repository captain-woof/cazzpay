{
  "name": "cazzpay-client",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "dev": "pnpm run typechain && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typechain": "cd ../contracts && npx hardhat clean && npx hardhat compile && find ./artifacts/contracts/ -maxdepth 5 -name '*.json' | grep -v \".dbg.json\" | xargs --replace={} cp \"{}\" ../client/contracts && cd ../client && cp -r ../contracts/typechain ./"
  },
  "dependencies": {
    "@paypal/react-paypal-js": "^7.7.0",
    "axios": "^0.27.2",
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@types/node": "17.0.31",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.3",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "ethers": "^5.6.6",
    "typescript": "4.6.4"
  }
}